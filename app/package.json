{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest -u --coverage --watchAll=false",
    "test:watch": "npm run test -- --watch",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "list:style": "stylelint \"**/*.{css,scss,sass,html,ts}\" --ignore-path .gitignore",
    "prettier": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix",
    "prepare": "cd .. && husky install app/.husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.1",
    "@react-native-segmented-control/segmented-control": "^2.4.0",
    "@types/styled-components-react-native": "^5.1.3",
    "install": "^0.13.0",
    "jest-scss-transform": "^1.0.3",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "react-native-vector-icons": "^9.1.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.66.24",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-jest": "^26.6.3",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.7.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
